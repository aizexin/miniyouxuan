<tabs tabs="{{tabs}}" bind:itemChange="itemChange">
  <view class="feedback_main">
    <view class="feedback_title">问题的种类</view>
    <view class="feedback_tips">
      <text>功能建议</text>
      <text>购买问题</text>
      <text>性能问题</text>
      <text>其他</text>
    </view>
    <view class="feedback_content">
      <textarea placeholder="请描述问题"></textarea>
      <view class="feedback_tool">
        <view class="upImageTool">
          <button>+</button>
          <ai-upImage class="upImageItem"></ai-upImage>
          <ai-upImage class="upImageItem"></ai-upImage>
          <ai-upImage class="upImageItem"></ai-upImage>
          <ai-upImage class="upImageItem"></ai-upImage>
          <ai-upImage class="upImageItem"></ai-upImage>
          <ai-upImage class="upImageItem"></ai-upImage>
        </view>
      </view>
    </view>
    <view class="form_submitButton_warp">
      <button>
        <icon type="success_no_circle" size="23" color="white"></icon>
        提交
      </button>
    </view>
  </view>
</tabs>