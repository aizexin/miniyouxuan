<ai-address-choose address="{{address}}" bindonClickGetAddress="onClickGetAddress">
</ai-address-choose>
<!-- tips -->
<view class="tips">
  <text>购物车</text>
</view>
<!-- 购物车内容 -->
<view class="cart_content">
  <view class="cart_list">
    <ai-cart-cell 
    class="cart_cell" 
    wx:for="{{cart}}"
    wx:for-index="index"
    wx:key="index"
    data="{{cart[index]}}"
    cell_index = "{{index}}"
    bindonClickItemCheck="onClickItemCheck"
    bindonClickChangeItemNum="onClickChangeItemNum"
    ></ai-cart-cell>
  </view>
</view>
<!-- 工具栏 -->
<ai-bottom-tool allChecked="{{allChecked}}"
totalNum="{{totalNum}}"
totalPrice="{{totalPrice}}"></ai-bottom-tool>