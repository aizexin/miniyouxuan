swiper {
  width: 750rpx;
  height: 340rpx;
}
swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cate image {
  width: 100%;
}
.index_floor .floor_title {
  padding: 10rpx 0;
}
.index_floor .floor_title image {
  width: 100%;
}
.index_floor .floor_list {
  overflow: hidden;
}
.index_floor .floor_list navigator {
  float: left;
  width: 33%;
  /* 后四个超链接 */
  /* 2 3 两个超链接 */
}
.index_floor .floor_list navigator:nth-last-child(-n+4) {
  /* 原图的宽高 232 *386 */
  height: 209rpx;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_list navigator:nth-child(2),
.index_floor .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_list navigator image {
  width: 100%;
  height: 100%;
}
