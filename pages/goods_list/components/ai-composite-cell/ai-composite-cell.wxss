.cell {
  border-bottom: 2rpx solid #ccc;
  display: flex;
}
.cell .leftImage {
  display: flex;
  flex: 2;
  justify-content: center;
  align-items: center;
}
.cell .leftImage image {
  width: 70%;
}
.cell .rightcontent {
  padding: 0 10rpx;
  flex: 3;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.cell .rightcontent .title {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.cell .rightcontent .price {
  font-size: 32rpx;
  color: var(--themeColor);
}
